# Environment Configuration Template
# Copy this file to .env and fill in your actual values

NODE_ENV=dev
NUXT_PUBLIC_APP_URL=http://localhost:3000

# SENDGRID Configuration
NUXT_PUBLIC_SITE_KEY=your_site_key_here
NUXT_SENDGRID_API_KEY=your_sendgrid_api_key_here
NUXT_SENDGRID_EMAIL_TEMPLATE_ID=your_template_id_here
NUXT_SENDGRID_FROM_EMAIL_ID=your_email@domain.com
NUXT_SENDGRID_SALES_TEAM_EMAILS=email1@domain.com,email2@domain.com
NUXT_GOOGLE_CAPTCHA_SECRET_KEY=your_captcha_secret_here

# Authentication
NUXT_JWT_TOKEN=your_jwt_token_here
NUXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
NUXT_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Microsoft/Teams Integration
NUXT_PUBLIC_MICROSOFT_APP_ID=your_microsoft_app_id_here
NUXT_MICROSOFT_APP_PASSWORD=your_microsoft_app_password_here
NUXT_PUBLIC_MICROSOFT_REDIRECT_URI=your_redirect_uri_here

# Slack Integration
NUXT_PUBLIC_SLACK_CLIENT_ID=your_slack_client_id_here
NUXT_SLACK_CLIENT_SECRET=your_slack_client_secret_here
NUXT_PUBLIC_SLACK_REDIRECT_URI=your_slack_redirect_uri_here

# Bot Endpoint
NUXT_PUBLIC_BOT_ENDPOINT=your_bot_endpoint_here

# AWS Configuration
NUXT_AWS_REGION=us-east-1
NUXT_AWS_ACCESS_KEY_ID=your_aws_access_key_here
NUXT_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
NUXT_AWS_BUCKET_NAME=your_bucket_name_here
NUXT_AWS_FOLDER_NAME=your_folder_name_here

# Database Configuration
NUXT_DB_HOST=localhost
NUXT_DB_PORT=5432
NUXT_DB_NAME=your_database_name
NUXT_DB_USER=your_username
NUXT_DB_PASSWORD=your_password

# Builder.io
VITE_PUBLIC_BUILDER_KEY=your_builder_key_here
